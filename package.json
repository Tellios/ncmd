{
  "name": "tellios-ncli",
  "version": "0.0.1",
  "description": "Helper scripts for easing workflow",
  "main": "index.js",
  "engines": {
    "node": ">= 6.10.3"
  },
  "bin": {
    "na": "./bin/alias",
    "ni": "./bin/node/ni",
    "nr": "./bin/node/nr",
    "nb": "./bin/node/nb",
    "nt": "./bin/node/nt",
    "nclone": "./bin/git/clone",
    "ncommit": "./bin/git/commit",
    "ncb": "./bin/git/createBranch",
    "ngcb": "./bin/git/getCurrentBranch",
    "ngb": "./bin/git/getBranches",
    "nmf": "./bin/git/mergeFrom",
    "nmt": "./bin/git/mergeTo",
    "nrb": "./bin/git/removeBranch",
    "nstatus": "./bin/git/status",
    "nsb": "./bin/git/switchBranch",
    "npull": "./bin/git/pull",
    "npush": "./bin/git/push",
    "nupstream": "./bin/git/setUpstream",
    "nd": "./bin/nd",
    "nk": "./bin/nk",
    "nsettings": "./bin/nsettings"
  },
  "scripts": {
    "prettier": "prettier --check --write \"{lib,src}/**/*.ts\"",
    "prettier:ci": "prettier -l \"{lib,src}/**/*.ts\"",
    "pretest": "npm run lint",
    "test": "jest",
    "test:watch": "jest --watchAll --runInBand",
    "build": "rimraf dist && tsc -p tsconfig.build.json",
    "build:watch": "rimraf dist && tsc --watch -p tsconfig.build.json",
    "setup": "npm ci && npm run prettier && npm run test && npm run build && npm link",
    "lint": "eslint . --cache --fix --ext .ts,.tsx"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "chalk": "^3.0.0",
    "cli-table2": "^0.2.0",
    "concurrently": "^5.0.0",
    "execa": "^3.4.0",
    "fs-extra": "^8.0.0",
    "inquirer": "^7.0.0",
    "inquirer-autocomplete-prompt": "^1.0.2",
    "js-yaml": "^3.13.1",
    "lodash": "^4.17.15",
    "readline-sync": "^1.4.9",
    "yargs": "^15.0.1"
  },
  "devDependencies": {
    "@types/cli-table2": "^0.2.2",
    "@types/fs-extra": "^8.0.0",
    "@types/inquirer": "6.5.0",
    "@types/jest": "^25.1.3",
    "@types/js-yaml": "^3.11.2",
    "@types/lodash": "^4.14.149",
    "@types/node": "^12.12.12",
    "@types/readline-sync": "^1.4.3",
    "@types/yargs": "^15.0.4",
    "@typescript-eslint/eslint-plugin": "^2.24.0",
    "@typescript-eslint/parser": "^2.24.0",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.10.0",
    "husky": "^4.2.3",
    "jest": "^25.1.0",
    "lint-staged": "^10.0.8",
    "prettier": "^1.19.1",
    "rimraf": "^3.0.0",
    "ts-jest": "^25.2.1",
    "typescript": "^3.8.3"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{ts,md}": "prettier --write",
    "*.{ts}": "eslint --check --max-warnings 0 --fix"
  }
}
