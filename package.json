{
  "name": "tellios-ncli",
  "version": "0.0.1",
  "description": "Helper scripts for easing workflow",
  "main": "index.js",
  "engines": {
    "node": ">= 6.10.3"
  },
  "bin": {
    "na": "./bin/alias",
    "ni": "./bin/node/ni",
    "nr": "./bin/node/nr",
    "nt": "./bin/node/nt",
    "nclone": "./bin/git/clone",
    "ncommit": "./bin/git/commit",
    "ncb": "./bin/git/createBranch",
    "ngcb": "./bin/git/getCurrentBranch",
    "ngb": "./bin/git/getBranches",
    "nmf": "./bin/git/mergeFrom",
    "nmt": "./bin/git/mergeTo",
    "nrb": "./bin/git/removeBranch",
    "nstatus": "./bin/git/status",
    "nsb": "./bin/git/switchBranch",
    "npull": "./bin/git/pull",
    "npush": "./bin/git/push",
    "nupstream": "./bin/git/setUpstream",
    "ndps": "./bin/docker/processes",
    "ndrm": "./bin/docker/remove",
    "ndstart": "./bin/docker/start",
    "ndstop": "./bin/docker/stop"
  },
  "scripts": {
    "pretty": "prettier --config .prettierrc.json --write \"{lib,src}/**/*.ts\"",
    "test": "jest",
    "test:watch": "jest --watchAll --runInBand",
    "build": "rimraf dist && tsc -p tsconfig.build.json",
    "build:watch": "rimraf dist && tsc --watch -p tsconfig.build.json",
    "setup": "npm ci && npm run pretty && npm run test && npm run build && npm link"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "chalk": "^2.4.1",
    "cli-table2": "^0.2.0",
    "concurrently": "^4.0.1",
    "execa": "^2.0.0",
    "fs-extra": "^8.0.0",
    "inquirer": "^7.0.0",
    "js-yaml": "^3.13.1",
    "lodash": "^4.17.11",
    "readline-sync": "^1.4.9",
    "yargs": "^15.0.1"
  },
  "devDependencies": {
    "@types/cli-table2": "^0.2.2",
    "@types/fs-extra": "^8.0.0",
    "@types/inquirer": "6.5.0",
    "@types/jest": "^24.0.12",
    "@types/js-yaml": "^3.11.2",
    "@types/lodash": "^4.14.119",
    "@types/node": "^12.0.0",
    "@types/readline-sync": "^1.4.3",
    "@types/sinon": "^7.0.11",
    "@types/yargs": "^13.0.0",
    "jest": "^24.7.1",
    "prettier": "^1.15.3",
    "rimraf": "^3.0.0",
    "sinon": "^7.1.1",
    "ts-jest": "^24.0.2",
    "typescript": "^3.6.3"
  },
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  }
}
